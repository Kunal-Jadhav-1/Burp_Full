{
    "version": 2,
    "builds": [
      {
        "src": "Backend/src/server.js", 
        "use": "@vercel/node"
      },
      {
        "src": "Frontend/src/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)", 
        "dest": "Backend/src/server.js"
      },
      {
        "src": "/(.*)", 
        "dest": "Frontend/dist/index.html"
      }
    ]
  }
  